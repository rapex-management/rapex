    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "critters": "^0.0.24",
    "formidable": "^3.5.4",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.9.14",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
